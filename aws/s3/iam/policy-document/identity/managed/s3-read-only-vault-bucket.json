{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "UserReadsVaultBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject*"
      ],
      "Resource": [
        "arn:aws:iam::fluxaugur:bucket/vault/com-nathanialmcconnell-vault",
        "arn:aws:iam::fluxaugur:bucket/vault/com-nathanialmcconnell-vault/*"
      ]
    }
  ]
}
