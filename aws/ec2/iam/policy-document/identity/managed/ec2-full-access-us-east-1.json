{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "UserFullAccessEC2USEast1",
      "Effect": "Allow",
      "Action": [
        "ec2:*"
      ],
      "Resource": [
        "*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:RequestedRegion": "us-east-1"
        }
      }
    }
  ]
}
